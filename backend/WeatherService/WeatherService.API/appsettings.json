{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenWeatherMap": {
    "BaseUrl": "https://api.openweathermap.org/data/2.5/weather"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Jwt": {
    "Issuer": "WeatherApp.AuthService",
    "Audience": "WeatherApp.Users"
  }
}